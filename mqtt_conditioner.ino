void setup() {
   pinMode(LED, OUTPUT);
  pinMode(D1,INPUT_PULLUP);
  Serial.begin(115200);
  IrSender.begin();
  WiFi.begin(ssid, password);
  while (WiFi.status() != WL_CONNECTED) 
  {
    delay(500);
    Serial.println("Connecting to WiFi..");
  }
  Serial.print("Connected to WiFi :");
  Serial.println(WiFi.SSID());
  client.setServer(mqtt_server, mqtt_port);
  client.setCallback(MQTTcallback);
  while (!client.connected()) 
  {
    Serial.println("Connecting to MQTT...");
    // if (client.connect("ESP8266", "username", "password")) <- uncomment this if your broker allow connect without password
    if (client.connect("ESP8266", "username", "password")) // <- and comment this
    {
      Serial.println("connected");
    }
    else
    {
      Serial.print("failed with state ");
      Serial.println(client.state());
      delay(2000);
    }
  }
  client.subscribe("lounge/conditioner");
}
void MQTTcallback(char* topic, byte* payload, unsigned int length) 
{
  Serial.print("Message received in topic: ");
  Serial.println(topic);
  Serial.print("Message:");
  String message;
  for (int i = 0; i < length; i++) 
  {
    message = message + (char)payload[i];
  }
  Serial.print(message);
  if (message == "on") {
      IrSender.sendRaw(onBtn, sizeof(onBtn) / sizeof(onBtn[0]), NEC_KHZ);
    } else if (message == "off") {
      IrSender.sendRaw(offBtn, sizeof(offBtn) / sizeof(offBtn[0]), NEC_KHZ);
